{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "demo-home": {
      "packageName": "@demo/home",
      "development": {
        "local": 3000,
        "fallback": "demo.sather.ws"
      }
    },
    "demo-shop": {
      "packageName": "@demo/shop",
      "development": {
        "local": 3001
      },
      "routing": [
        {
          "paths": ["/shop", "/shop/:path*"]
        }
      ]
    }
  }
}
